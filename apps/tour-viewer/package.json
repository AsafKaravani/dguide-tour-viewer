{
  "name": "tour-viewer",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --host",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-and-fix": "eslint . --ext .ts,.tsx --fix",
    "plop": "plop",
    "preview": "vite preview",
    "storybook": "start-storybook -s ./src/assets -p 6006",
